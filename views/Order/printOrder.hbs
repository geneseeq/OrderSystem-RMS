<div class="container " ng-controller="orderDetailController">
    <div>
        <div class="panel panel-success">
            <div class="panel panel-heading">
                <h3 class="panel-title">订单基本信息</h3>
            </div>
            <div class="row   p-relative">
                <div class="col-sm-6 col-sm-offset-1">
                    <table class="table table-striped table-bordered table-condensed table-responsive animated bounceInDown ">
                        <tr>
                            <td>订单编号</td>
                            <td>[[detail.OrderID]]</td>
                        </tr>
                        <tr>
                            <td>项目编号</td>
                            <td>[[detail.ProgrameID]]</td>
                        </tr>
                        <tr>
                            <td>订单类型</td>
                            <td>[[detail.OrderType]]</td>
                        </tr>
                        <tr>
                            <td>下单人姓名</td>
                            <td>[[detail.UserName]]</td>
                        </tr>
                        <tr>
                            <td>联系电话</td>
                            <td>[[detail.Phone]]</td>
                        </tr>
                        <tr>
                            <td>所在机构</td>
                            <td>[[detail.Instituion]]</td>
                        </tr>
                        <tr>
                            <td>收货地址</td>
                            <td>[[detail.ReceiveAddress]]</td>
                        </tr>
                        <tr>
                            <td>运单号</td>
                            <td>[[detail.ReceiveWayBill]]</td>
                        </tr>
                    </table>
                </div>
                        <div id="code">
             </div>
            </div>
    
            <div class="row service-row">
                    <div  ng-repeat='iteam in samples' class="col-sm-3 col-lg-3 col-md-3 col-sm-offset-1 text-center">
                        <div class="animated pulse">
                            <div class="avatar " id="[[iteam._id]]">
                                
                            </div>
                            <h5>订单编号:[[iteam.OrderID]]</h5>
                            <h5>样本ID:[[iteam.SampleID]]</h5>
                        </div>
                    </div>
               
            </div>
            <div class="form-group text-center">
                 <a class="btn btn-primary btn-lg" ng-click="print()">打印</a>
            </div>
        </div>
    </div>
    
</div>
